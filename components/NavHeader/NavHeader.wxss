.header {
  padding-bottom: 20rpx;
}
.header .title {
  font-style: 32rpx;
  color: #666;
  line-height: 80rpx;
}
.header .content {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header .content .more {
  font-size: 26rpx;
  padding: 10rpx 20rpx;
  border: 1rpx solid #333;
  border-radius: 30rpx;
}
